{
  "name": "falkordb-canvas",
  "version": "1.0.0",
  "description": "A standalone web component for visualizing FalkorDB graphs using force-directed layouts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsc --watch",
    "example": "python3 -m http.server 8080",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "falkordb",
    "graph",
    "visualization",
    "web-component",
    "force-directed",
    "canvas"
  ],
  "author": "FalkorDB",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^19.2.7",
    "d3": "^7.9.0",
    "force-graph": "^1.44.4",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
